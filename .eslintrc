{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "moment": true
  },
  "rules": {
    "semi": [1, "always"],
    "no-debugger": 1,
    "no-unused-vars": 1,
    "angular/on-watch": 1
  }
}
